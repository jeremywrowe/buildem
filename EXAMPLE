$configuration.workers = 2

run "initialize.sh"
run "clean.sh"

unorderd do
  queued_run "build.sh"
  queued_run "checkin.sh"
  queued_run "publish.sh"
  run "some_task_in_the_middle_of_queue.sh" # this will block all of the queued_run calls after this
  queued_run "build.sh"
  queued_run "checkin.sh"
  queued_run "publish.sh"
end

run "do_something_at_the_end.sh"